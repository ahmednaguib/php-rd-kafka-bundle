{
	"name" : "mshauneu/php-rdkafka-bundle",
	"type" : "library",
	"description" : "Integrates php-rdkafka with Symfony2|3",
	"license" : "MIT",
	"authors" : [{
			"name" : "Mikhail Shauneu"
		}
	],
	"keywords" : [
		"php",
		"symfony",
		"kafka"
