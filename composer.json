		"symfony/yaml" : "~2.3 || ~3.0",
		"kwn/php-rdkafka-stubs" : "^0.0.4",
		"ext-rdkafka" : "*"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4.8 || ~5.0"
	}
}